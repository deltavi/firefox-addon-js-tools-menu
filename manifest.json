{
  "description": "Firefox Webextensions - JSTools Menù",
  "manifest_version": 2,
  "name": "JSTools Menù",
  "version": "1.0",
  "homepage_url": "http://www.vincenzodevivo.com",
  "icons": {
    "16": "icons/btn_magic_hat_js.16x16.png",
    "20": "icons/btn_magic_hat_js.20x20.png",
    "24": "icons/btn_magic_hat_js.24x24.png",
    "32": "icons/btn_magic_hat_js.32x32.png",
    "48": "icons/btn_magic_hat_js.48x48.png",
    "64": "icons/btn_magic_hat_js.64x64.png",
    "128": "icons/btn_magic_hat_js.128x128.png"
  },
  "browser_action": {
    "default_icon": "icons/btn_magic_hat_js.32x32.png",
    "default_title": "JSTools Menù",
    "default_popup": "popup/menu.html"
  },
  "options_ui": {
    "page": "settings/options.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "applications": {
    "gecko": {
      "id": "firefox-addon-js-tools-menu@vincenzodevivo.com",
      "strict_min_version": "42.0"
    }
  }
}